{
    "name": "better-auth",
    "version": "0.5.2-beta.3",
    "description": "The most comprehensive authentication library for TypeScript.",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/better-auth/better-auth",
        "directory": "packages/better-auth"
    },
    "scripts": {
        "build": "NODE_OPTIONS='--max-old-space-size=4000' tsup --clean --dts",
        "dev": "NODE_OPTIONS='--max-old-space-size=4000' tsup --watch --sourcemap",
        "dev:dts": "NODE_OPTIONS='--max-old-space-size=8192' tsup --watch --dts",
        "test": "pnpm prisma:push && vitest",
        "prisma:push": "prisma db push --schema src/adapters/prisma-adapter/test/schema.prisma",
        "test:adapters": "pnpm prisma:push && vitest adapters",
        "bump": "bumpp",
        "typecheck": "tsc --noEmit"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./social-providers": {
            "import": "./dist/social.js",
            "require": "./dist/social.cjs"
        },
        "./client": {
            "import": "./dist/client.js",
            "require": "./dist/client.cjs",
            "types": "./dist/client.d.ts"
        },
        "./client/plugins": {
            "import": "./dist/client/plugins.js",
            "require": "./dist/client/plugins.cjs",
            "types": "./dist/client/plugins.d.ts"
        },
        "./types": {
            "import": "./dist/types.js",
            "require": "./dist/types.cjs"
        },
        "./crypto": {
            "import": "./dist/crypto.js",
            "require": "./dist/crypto.cjs"
        },
        "./cookies": {
            "import": "./dist/cookies.js",
            "require": "./dist/cookies.cjs"
        },
        "./oauth2": {
            "import": "./dist/oauth2.js",
            "require": "./dist/oauth2.cjs"
        },
        "./react": {
            "import": "./dist/react.js",
            "require": "./dist/react.cjs"
        },
        "./preact": {
            "import": "./dist/preact.js",
            "require": "./dist/preact.cjs"
        },
        "./solid": {
            "import": "./dist/solid.js",
            "require": "./dist/solid.cjs"
        },
        "./api": {
            "import": "./dist/api.js",
            "require": "./dist/api.cjs"
        },
        "./db": {
            "import": "./dist/db.js",
            "require": "./dist/db.cjs"
        },
        "./vue": {
            "import": "./dist/vue.js",
            "require": "./dist/vue.cjs"
        },
        "./plugins": {
            "import": "./dist/plugins.js",
            "require": "./dist/plugins.cjs"
        },
        "./plugins/access": {
            "import": "./dist/access.js",
            "require": "./dist/access.cjs"
        },
        "./svelte-kit": {
            "import": "./dist/svelte-kit.js",
            "require": "./dist/svelte-kit.cjs"
        },
        "./solid-start": {
            "import": "./dist/solid-start.js",
            "require": "./dist/solid-start.cjs"
        },
        "./svelte": {
            "import": "./dist/svelte.js",
            "require": "./dist/svelte.cjs"
        },
        "./next-js": {
            "import": "./dist/next-js.js",
            "require": "./dist/next-js.cjs"
        },
        "./package.json": "./package.json",
        "./node": {
            "import": "./dist/node.js",
            "require": "./dist/node.cjs"
        },
        "./adapters/prisma": {
            "import": "./dist/adapters/prisma.js",
            "require": "./dist/adapters/prisma.cjs"
        },
        "./adapters/drizzle": {
            "import": "./dist/adapters/drizzle.js",
            "require": "./dist/adapters/drizzle.cjs"
        },
        "./adapters/mongodb": {
            "import": "./dist/adapters/mongodb.js",
            "require": "./dist/adapters/mongodb.cjs"
        },
        "./adapters/kysely": {
            "import": "./dist/adapters/index.js",
            "require": "./dist/adapters/index.cjs"
        }
    },
    "devDependencies": {
        "@prisma/client": "^5.19.1",
        "@simplewebauthn/types": "^10.0.0",
        "@types/better-sqlite3": "^7.6.11",
        "@types/pg": "^8.11.6",
        "@types/prompts": "^2.4.9",
        "@types/react": "^18.3.3",
        "better-sqlite3": "^11.3.0",
        "drizzle-orm": "^0.33.0",
        "happy-dom": "^15.7.4",
        "hono": "^4.5.4",
        "listhen": "^1.7.2",
        "mongodb": "^6.9.0",
        "mysql2": "^3.11.0",
        "next": "^14.2.8",
        "oauth2-mock-server": "^7.1.2",
        "pg": "^8.12.0",
        "prisma": "^5.19.1",
        "react": "^18.3.1",
        "solid-js": "^1.8.18",
        "tsup": "^8.2.4",
        "typescript": "5.6.1-rc",
        "vitest": "^1.6.0",
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@better-fetch/fetch": "^1.1.9",
        "@nanostores/query": "^0.3.4",
        "@nanostores/react": "^0.7.3",
        "@nanostores/solid": "^0.4.2",
        "@nanostores/vue": "^0.10.0",
        "@noble/ciphers": "^0.6.0",
        "@noble/hashes": "^1.5.0",
        "@simplewebauthn/browser": "^10.0.0",
        "@simplewebauthn/server": "^10.0.1",
        "better-call": "0.2.7",
        "consola": "^3.2.3",
        "defu": "^6.1.4",
        "kysely": "^0.27.4",
        "nanoid": "^5.0.7",
        "nanostores": "^0.11.2",
        "oslo": "^1.2.1",
        "jose": "^5.9.4",
        "zod": "^3.22.5"
    },
    "files": [
        "dist/*",
        "./package.json"
    ]
}
